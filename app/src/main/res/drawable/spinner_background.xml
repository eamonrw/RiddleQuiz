<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center_vertical|right" android:right="15dp">
        <layer-list>
            <item android:gravity="center">
                <rotate
                    android:fromDegrees="45"
                    android:toDegrees="45">
                    <shape android:shape="line">
                        <stroke android:width="2dp"/>
                        <size android:width="12dp" android:height="12dp" />
                        <solid android:color="#000000" />
                    </shape>
                </rotate>
            </item>
            <item android:gravity="center" android:left="11.5dp">
                <rotate
                    android:fromDegrees="135"
                    android:toDegrees="45">
                    <shape android:shape="line">
                        <stroke android:width="2dp"/>
                        <size android:width="12dp" android:height="12dp" />
                        <solid android:color="#000000" />
                    </shape>
                </rotate>
            </item>
            <item android:gravity="center" android:right="40dp">
                <shape android:shape="rectangle">
                    <size android:width="1.5dp" android:height="25dp" />
                    <solid android:color="#000000" />
                </shape>
            </item>
            <item android:bottom="21dp" android:gravity="center">
                <shape android:shape="rectangle">
                    <size android:width="30dp" android:height="10dp" />
                </shape>
            </item>
        </layer-list>
    </item>
</layer-list>